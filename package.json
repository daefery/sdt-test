{
  "name": "sdt-test",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node --watch src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --updateSnapshot --detectOpenHandles",
    "swagger-autogen": "node swagger.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daefery/sdt-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daefery/sdt-test/issues"
  },
  "homepage": "https://github.com/daefery/sdt-test#readme",
  "dependencies": {
    "and": "^0.0.3",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "luxon": "^3.4.4",
    "node-cron": "^3.0.3",
    "sequelize": "^6.37.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {},
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**"
    ],
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/index.js"
    ]
  }
}
